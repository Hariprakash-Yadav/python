def auth(user,pwd):
    
    if user == 'abc':
      if pwd == 'hari':
        print('Login Successfully')
      else:
        print('Incorrect Password')
    elif user == 'xyz':
      if pwd == 'poorna':
        print('Login Successfully')
      else:
        print('Incorrect Password')
    else:
        print('Incorrect User')
        
auth('hjdsbdkj','hari')
